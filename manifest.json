{
    "name": "Gingr",
    "version": "0.2.0",
    "description": "Enhancement Lingr",
    "permissions": ["*://*.lingr.com/*"],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "content_scripts": [{
        "matches": ["*://*.lingr.com/"],
        "css": ["extensions.css"]
    }],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2
}